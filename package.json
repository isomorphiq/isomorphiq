{
	"name": "testingg",
	"version": "1.0.0",
	"description": "",
	"type": "module",
	"main": "index.js",
	"workspaces": [
		"packages/*",
		"packages/api-prelude",
		"packages/http-server",
		"packages/plugins/plugins/*",
		"packages/persistence-*",
		"services/*",
		"services/gateway",
		"services/workflows",
		"packages/supervisor",
		"apps/*",
		"web"
	],
	"scripts": {
		"build": "rsbuild build",
		"build:watch": "rsbuild dev",
		"typecheck": "yarn workspaces foreach -ptA run typecheck",
		"lint": "yarn workspaces foreach -ptA run lint",
		"test": "yarn workspaces foreach -ptA run test",
		"start-mcp": "yarn workspace @isomorphiq/mcp start",
		"daemon": "yarn workspace @isomorphiq/daemon start",
		"gateway": "yarn workspace @isomorphiq/gateway start",
		"web": "rsbuild dev",
		"start:dev": "bash -c \"export DB_PATH=$(pwd)/db; export DAEMON_HTTP_PORT=3004; export GATEWAY_PORT=3003; yarn run start-mcp & yarn run daemon & yarn run gateway & yarn run web; wait\"",
		"start": "yarn run start:dev",
		"format": "biome format --write .",
		"clean": "rm -rf dist"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"engines": {
		"node": ">=24.0.0"
	},
	"packageManager": "yarn@4.6.0",
